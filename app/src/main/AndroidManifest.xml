<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tbadhit.webview">

    <uses-permission android:name="android.permission.INTERNET" />

    <!--(ICON) Ubah yang roundIcon & icon-->
    <!-- END OF CUSTOM ICON APLIKASI (lanjut custom language)-->
    <!-- Kembali ke "MainActivity" cek line 110-->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_calculator_human"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_calculator_human_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WebView">
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.WebView.NoActionBar"></activity>
    </application>

</manifest>